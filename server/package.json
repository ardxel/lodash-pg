        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.7.3"
    },
    "dependencies": {
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "typescript-eslint": "^7.7.1"
    }
}
